{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": [
        "./client/app/*"
      ],
      "@environments/*": [
        "./client/environments/*"
      ]
    }
  },
  "include": [
    "client/**/*.ts"
  ],
  "exclude": [
    "client/test.ts",
    "client/**/*.spec.ts"
  ]
}